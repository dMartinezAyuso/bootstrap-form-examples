{
  "bitwise": true,
  "browser": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "esnext": true,
  "globalstrict": true,
  "globals": {
    "angular"   : false,
    "after"     : false,
    "afterEach" : false,
